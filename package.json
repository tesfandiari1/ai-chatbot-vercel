{
  "name": "ai-chatbot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "predev": "node scripts/services/ensure-port-3000.js",
    "dev": "concurrently \"pnpm dev:css\" \"pnpm dev:next\"",
    "dev:full": "npm-run-all --parallel dev:css dev:next",
    "dev:next": "npx next dev -p 3000",
    "dev:css": "npx tailwindcss -i ./css/tailwind.css -o ./public/styles/tailwind.css --watch",
    "dev:turbo": "npm-run-all --parallel dev:css dev:next:turbo",
    "dev:next:turbo": "next dev --turbo -p 3000",
    "dev:all": "concurrently \"npm run dev\" \"npm run start:mcp\"",
    "dev:complete": "node scripts/services/start-app.js",
    "build": "npm-run-all build:css build:next",
    "build:next": "tsx lib/db/migrate && next build",
    "build:css": "NODE_ENV=production npx tailwindcss -i ./css/tailwind.css -o ./public/styles/tailwind.css --minify",
    "prestart": "node scripts/services/ensure-port-3000.js",
    "start": "next start -p 3000",
    "start:mcp": "node scripts/mcp/test-mcp-local.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:mcp\"",
    "clean": "node scripts/services/clean-project.js",
    "clean:full": "node scripts/services/clean-project.js --full",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "services:check": "node scripts/services/service-manager.js",
    "services:start": "node scripts/services/start-app.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "test": "export PLAYWRIGHT=True && pnpm exec playwright test --workers=4",
    "setup": "pnpm install && pnpm db:migrate",
    "start:dev": "pnpm dev",
    "start:dev:full": "pnpm dev:complete",
    "start:prod": "pnpm build && pnpm start",
    "reset": "pnpm clean:full && pnpm setup"
  },
